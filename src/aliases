alias l='ls'
alias ll='ls -l'
alias la='ls -a'
alias lt='du -sh * | sort -h'
alias lt1='ls -s -S -1 -c'

alias q='cd ..'
alias qq='cd ../../'
alias qqq='cd ../../../'
alias qqqq='cd ../../../../'
alias qqqqq='cd ../../../../../'

alias gh='history | grep'

alias tree='tree -C'

alias mnt='mount | grep -E ^/dev | column -t'
alias va='source .venv/bin/activate'

alias gitdiff='diff -u --color'

